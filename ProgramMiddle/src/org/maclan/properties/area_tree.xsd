	    						maxOccurs="unbounded">
	    						<xsd:complexType>
	    							<xsd:attribute name="versionId"
	    								type="xsd:long" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute name="alias"
	    								type="xsd:string" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute name="descriptionId"
	    								type="xsd:long" use="required">
	    							</xsd:attribute>
	    						</xsd:complexType>
	    					</xsd:element>
	    				</xsd:sequence>
	    			</xsd:complexType>
	    		</xsd:element>
