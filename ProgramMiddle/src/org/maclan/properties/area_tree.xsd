    	</xsd:sequence>
    	<xsd:attribute name="id" type="xsd:long" use="required"></xsd:attribute>
    	<xsd:attribute name="extensionAreaId" type="xsd:long"></xsd:attribute>
    	<xsd:attribute name="alias" type="xsd:string"></xsd:attribute>
    </xsd:complexType>


</xsd:schema>