	    		<xsd:element name="ResourceRef" maxOccurs="1"
	    			minOccurs="1">
	    			<xsd:complexType>
	    				<xsd:sequence>
	    					<xsd:element name="Record"
	    						maxOccurs="unbounded" minOccurs="0">
	    						<xsd:complexType>
	    							<xsd:attribute name="text"
	    								type="xsd:string">
