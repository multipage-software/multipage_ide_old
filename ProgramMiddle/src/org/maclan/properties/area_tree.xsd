	    							<xsd:attribute name="enumerationId"
	    								type="xsd:long" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute name="description"
	    								type="xsd:string" use="required">
	    							</xsd:attribute>
	    						</xsd:complexType>
	    					</xsd:element>
	    				</xsd:sequence>
	    			</xsd:complexType>
	    		</xsd:element>
	    		<xsd:element name="EnumerationValue" minOccurs="1"
	    			maxOccurs="1">
	    			<xsd:complexType>
	    				<xsd:sequence>
	    					<xsd:element name="Record" minOccurs="0"
	    						maxOccurs="unbounded">
	    						<xsd:complexType>
	    							<xsd:attribute name="enumerationId"
	    								type="xsd:long" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute
	    								name="enumerationValueId" type="xsd:long" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute name="value"
