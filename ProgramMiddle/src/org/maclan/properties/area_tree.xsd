	    							<xsd:attribute name="revision"
	    								type="xsd:long">
	    							</xsd:attribute>
	    							<xsd:attribute name="created"
	    								type="xsd:string">
	    							</xsd:attribute>
	    							<xsd:attribute name="textValue"
	    								type="xsd:string">
	    							</xsd:attribute>
	    							<xsd:attribute name="realValue"
	    								type="xsd:double">
	    							</xsd:attribute>
	    							<xsd:attribute
	    								name="localizedTextValueId" type="xsd:long">
	    							</xsd:attribute>
	    							<xsd:attribute name="integerValue"
	    								type="xsd:long">
	    							</xsd:attribute>
	    							<xsd:attribute name="areaId"
	    								type="xsd:long" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute name="access"
	    								type="xsd:string" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute name="hidden"
	    								type="xsd:string" use="required">
	    							</xsd:attribute>
	    							<xsd:attribute name="id"
